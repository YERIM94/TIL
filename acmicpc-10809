백준알고리즘
10809번 알파벳찾기

import java.util.Scanner;

public class hi {
	public static void main(String[] args){
		Scanner scan = new Scanner(System.in);
		
		String word = scan.nextLine();
		int[] array = new int[26];
		
		for(int i=0;i<26;i++){
			array[i] = word.indexOf('a'+i);
			System.out.print(array[i]+" ");
		}
	}
}
